#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/kotow/projects/articles/vue-modal-trap-focus/node_modules/.pnpm/esbuild@0.13.8/node_modules/esbuild/bin/node_modules:/mnt/c/Users/kotow/projects/articles/vue-modal-trap-focus/node_modules/.pnpm/esbuild@0.13.8/node_modules/esbuild/node_modules:/mnt/c/Users/kotow/projects/articles/vue-modal-trap-focus/node_modules/.pnpm/esbuild@0.13.8/node_modules:/mnt/c/Users/kotow/projects/articles/vue-modal-trap-focus/node_modules/.pnpm/node_modules:/mnt/c/Users/kotow/projects/articles/vue-modal-trap-focus/node_modules:/mnt/c/Users/kotow/projects/articles/node_modules:/mnt/c/Users/kotow/projects/node_modules:/mnt/c/Users/kotow/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/kotow/projects/articles/vue-modal-trap-focus/node_modules/.pnpm/esbuild@0.13.8/node_modules/esbuild/bin/node_modules:/mnt/c/Users/kotow/projects/articles/vue-modal-trap-focus/node_modules/.pnpm/esbuild@0.13.8/node_modules/esbuild/node_modules:/mnt/c/Users/kotow/projects/articles/vue-modal-trap-focus/node_modules/.pnpm/esbuild@0.13.8/node_modules:/mnt/c/Users/kotow/projects/articles/vue-modal-trap-focus/node_modules/.pnpm/node_modules:/mnt/c/Users/kotow/projects/articles/vue-modal-trap-focus/node_modules:/mnt/c/Users/kotow/projects/articles/node_modules:/mnt/c/Users/kotow/projects/node_modules:/mnt/c/Users/kotow/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/esbuild" "$@"
else
  exec node  "$basedir/../../bin/esbuild" "$@"
fi
